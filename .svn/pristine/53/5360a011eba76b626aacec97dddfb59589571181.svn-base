using System;

namespace Model
{
    public class ActionLog
    {
        public long logId { get; set; }
        public string name { get; set; }
        public ActionName actionName { get; set; }
        public string actionBy { get; set; }
        public DateTime actionDate { get; set; }
    }

    public enum ActionName
    {
        Inserted,
        Updated,
        Deleted,
        Printed
    }
}
